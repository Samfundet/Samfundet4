    "postcss": "^8.4.31",
    "react": "^18.2.0",
    "react-cookie": "^6.1.1",
    "react-dom": "^18.2.0",
    "react-helmet-async": "^1.3.0",
    "react-loading-skeleton": "^3.3.1",
    "react-markdown": "^9.0.0",
    "react-modal": "^3.16.1",
    "react-refresh": "^0.14.0",
    "react-router-dom": "^6.16.0",
    "react-toastify": "^9.1.3",
